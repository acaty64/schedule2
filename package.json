        "babel-loader": "^7.1.2",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-3": "^6.24.1",
        "bootstrap-sass": "^3.3.7",
        "canvas": "^2.5.0",
        "cross-env": "^5.0.5",
        "file-loader": "^1.1.4",
        "flush-promises": "^1.0.2",
        "jest": "^22.4.4",
        "jquery": "^3.2",
        "jsdom": "^15.1.1",
